# Sample Service Environment Configuration

SERVICE_NAME=sample-service

# Service port (externally exposed)
PORT=3001

# Application settings
NODE_ENV=development

# Service Dependencies - Local Development
SERVICE_REGISTRY_URL=http://localhost:3000
IAM_URL=http://localhost:3002
PUBSUB_EMULATOR_HOST=localhost:8085

# Service Dependencies - Cloud Run
# SERVICE_REGISTRY_URL=http://service-registry:3000
# IAM_URL=http://iam-service:3000
# PUBSUB_EMULATOR_HOST=pubsub-emulator:8085

# PubSub Emulator
PUBSUB_PROJECT_ID=city-services

# Database Configuration
DB_HOST=localhost
DB_PORT=5433
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=sample_service

# Logging
LOG_LEVEL=info 